{
  "name": "dbus-next",
  "version": "0.1.0",
  "keywords": [
    "dbus",
    "dcop",
    "d-bus",
    "rpc",
    "gnome",
    "kde"
  ],
  "description": "The next great DBus library for Node",
  "files": [
    "lib/*",
    "index.js",
    "package.json"
  ],
  "directories": {
    "lib": "lib",
    "test": "test",
    "examples": "examples"
  },
  "main": "index.js",
  "maintainers": [
    {
      "name": "Tony Crisci",
      "email": "tony@dubstepdish.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/acrisci/node-dbus-next.git"
  },
  "dependencies": {
    "event-stream": "3.3.4",
    "hexy": "^0.2.10",
    "jsbi": "^2.0.5",
    "put": "0.0.6",
    "safe-buffer": "^5.1.1",
    "xml2js": "^0.4.17"
  },
  "optionalDependencies": {
    "abstract-socket": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-syntax-bigint": "^7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "commander": "^2.19.0",
    "jest": "^23.6.0"
  },
  "scripts": {
    "integration": "dbus-run-session -- jest ./test/integration",
    "coverage": "dbus-run-session -- jest --coverage",
    "unit": "jest ./test/*.test.js",
    "test": "dbus-run-session -- jest"
  },
  "engine": {
    "node": ">=8.2.1"
  }
}
